    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Entrée0_ProcessInputRow(Entrée0Buffer Row)
    {
        CultureInfo frenchCulture = new CultureInfo("fr-BE");
        CultureInfo englishCulture = new CultureInfo("en-US");

        Row.DayEnglishName = Row.DateInput.ToString("dddd", englishCulture);
        Row.DayFrenchName = Row.DateInput.ToString("dddd", frenchCulture);
        Row.MonthEnglishName = Row.DateInput.ToString("MMMM", englishCulture);
        Row.MonthFrenchName = Row.DateInput.ToString("MMMM", frenchCulture);

        Row.WeekNumber = (uint) englishCulture.Calendar.GetWeekOfYear(Row.DateInput, CalendarWeekRule.FirstDay, DayOfWeek.Monday);
        Row.DayOfWeekNumber = (uint)Row.DateInput.DayOfWeek + 1; 
        Row.DayOfYearNumber = (uint) Row.DateInput.DayOfYear;

        Row.DateKey = ulong.Parse(Row.DateInput.ToString("yyyyMMdd"));
    }

